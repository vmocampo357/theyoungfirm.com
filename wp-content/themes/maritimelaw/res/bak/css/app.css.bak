#jal-ul-element-filters
{
    background-color:rgba(0,0,0,0.02);
    box-shadow:inset 0px 0px 10px -3px rgba(0,0,0,0.2);
    padding:10px;
}
#jal-ul-element-filters li:hover
{
    background-color:#27597c;
    color:white;
}
#jal-ul-element-filters li label
{
}
#jal-ul-element-filters li
{
    display:block;
    padding:10px 10px 10px 35px;
    background-color:rgba(0,0,0,0.05);
    margin-bottom:2px;
    display:inline-block;
    cursor:pointer;
    width:100%;
    text-transform: uppercase;
    font-size:12px;
    font-weight:700;
    position:relative;
}
#jal-ul-element-filters li span:after
{
    content:"";
    position:absolute;
    display:none;
}
#jal-ul-element-filters li input:checked ~ span
{
    background-color:#27597c;
}
#jal-ul-element-filters li input:checked ~ span:after
{
    display:block;
}
#jal-ul-element-filters li input
{
    position:absolute;
    opacity:0;
    cursor: pointer;
}
#jal-ul-element-filters li span:after
{
    left: 10px;
    top: 6px;
    width: 5px;
    height: 10px;
    border: solid white;
    border-width: 0 3px 3px 0;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
}
#jal-ul-element-filters li span
{
    position: absolute;
    top: 5px;
    left: 5px;
    height: 25px;
    width: 25px;
    background-color: #eee;
    cursor: pointer;
}
