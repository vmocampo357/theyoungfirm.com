/* Arrows */
.slick-prev-jal i,
.slick-next-jal i{
    font-size: 40px !important;
}
.slick-prev-jal,
.slick-next-jal
{
    font-size: 40px !important;
    line-height: 0;
    position: absolute;
    top: 50%;
    display: block;
    width: 50px;
    height: 50px;
    padding: 0;
    -webkit-transform: translate(0, -50%);
    -ms-transform: translate(0, -50%);
    transform: translate(0, -50%);
    cursor: pointer;
    color: rgba(0,0,0,0.25);
    border: none;
    outline: none;
    background: transparent;
}
.slick-prev-jal:hover,
.slick-prev-jal:focus,
.slick-next-jal:hover,
.slick-next-jal:focus
{
    /*color: transparent;
    outline: none;
    background: transparent;*/
    opacity:0.75;
}
.slick-prev-jal:hover:before,
.slick-prev-jal:focus:before,
.slick-next-jal:hover:before,
.slick-next-jal:focus:before
{
    opacity: 1;
}
.slick-prev-jal.slick-disabled:before,
